{
  "expo": {
    "name": "OnMovieApp",
    "slug": "OnMovieApp",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesIcloudStorage": true,
      "bundleIdentifier": "com.onmovie.onmovieapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "plugins": [
      "expo-document-picker",
      "expo-media-library",
      "expo-notifications"
    ],
    "android": {
      "package": "com.onmovie.onmovieapp",
      "googleServicesFile": "./google-services.json"
    }
  }
}
